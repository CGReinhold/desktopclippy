{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "arrow-body-style": 0,
      "max-len": 0,
      "no-underscore-dangle": 0,
      "global-require": 0,
      "no-throw-literal": 0,
      "array-callback-return": 0,
      "no-param-reassign": 0,
      "no-return-assign": 0,
      "no-nested-ternary": 0,
      "camelcase": 0
  }
}